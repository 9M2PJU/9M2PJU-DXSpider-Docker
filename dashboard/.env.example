# DXSpider Dashboard Configuration
# Copy this file to ../.env and adjust values as needed

# Dashboard Port
DASHBOARD_PORT=8080

# Maximum number of spots to return per API call
DASHBOARD_MAX_SPOTS=100

# Cache TTL in seconds (how long to cache API responses)
DASHBOARD_CACHE_TTL=5

